		<div class="bg-base-200 rounded-lg max-w-[1024px] w-full mx-auto p-4 md:p-8">
			<div class="flex flex-col lg:flex-row gap-8 items-center">
				<div class="flex flex-col space-y-8 md-content">
					<slot />
				</div>
			</div>
		</div>
	</div>
</Layout>